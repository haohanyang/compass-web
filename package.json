{
  "name": "compass-web",
  "version": "0.2.6",
  "bin": "app.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "ELECTRON_OVERRIDE_DIST_PATH=/dev/null ELECTRON_SKIP_BINARY_DOWNLOAD=1 compass/node_modules/.bin/webpack",
    "build:production": "ELECTRON_OVERRIDE_DIST_PATH=/dev/null ELECTRON_SKIP_BINARY_DOWNLOAD=1 compass/node_modules/.bin/webpack --mode production",
    "start": "node app.js"
  },
  "author": "Haohan Yang",
  "license": "SSPL",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haohanyang/compass-web.git"
  },
  "homepage": "https://github.com/haohanyang/compass-web#readme",
  "bugs": {
    "url": "https://github.com/haohanyang/compass-web/issues"
  },
  "description": "MongoDB Compass that runs on a browser",
  "dependencies": {
    "@fastify/auth": "^5.0.2",
    "@fastify/basic-auth": "^6.2.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/csrf-protection": "^7.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@fastify/view": "^11.1.0",
    "@fastify/websocket": "^11.1.0",
    "bson": "^6.10.4",
    "compass-preferences-model": "file:./vendors/compass-preferences-model",
    "debug": "file:./vendors/debug",
    "eta": "^3.5.0",
    "fastify": "^5.3.3",
    "hadron-document": "file:./vendors/hadron-document",
    "lodash": "^4.17.21",
    "mongodb": "^6.17.0",
    "mongodb-connection-string-url": "^3.0.2",
    "mongodb-ns": "file:./vendors/mongodb-ns",
    "mongodb-schema": "^12.5.2",
    "node-cache": "^5.1.2",
    "papaparse": "^5.3.2",
    "stream-json": "^1.7.5",
    "strip-bom-stream": "^4.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^13.0.0",
    "dotenv": "^16.5.0"
  },
  "pnpm": {},
  "keywords": [
    "mongodb",
    "compass",
    "database",
    "GUI"
  ],
  "packageManager": "pnpm@10.12.1"
}
